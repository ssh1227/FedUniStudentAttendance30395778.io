<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.edu.Federation.itech.studentattendentances30395778">
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.OpeningActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity"
            android:label="Login"/>
        <activity android:name=".activity.RegisterActivity"
            android:label="Register"/>
        <activity android:name=".activity.AddCourseActivity"
            android:label="Add Course/Unit"/>
        <activity android:name=".activity.AddStudentActivity"
            android:label="Add Student"/>
        <activity android:name=".activity.StudentActivity"
            android:label="Student"/>
        <activity android:name=".activity.AttendanceActivity"
            android:label="Attendance"/>
        <activity android:name=".activity.MainActivity"
            android:label="@string/app_name"/>
    </application>

</manifest>